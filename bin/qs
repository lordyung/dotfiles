#!/bin/bash

if [ $#  = 1  ]; then
    url=ot2jmekaj.bkt.clouddn.com/$1
elif [ $# = 2  ];then
    url=$(qshell domains $1)/$2
else
    echo Usuage: qs \(bucket\) [key]
fi

wget -T 15 --tries=1 --spider $url > /dev/null 2>&1 && echo $url && exit

qshell fput distfiles $1 $1 && exit
echo http://ot2jmekaj.bkt.clouddn.com/$1 


